<div class="page-header">
  <h1>Listing physical_items</h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Id</th>
      <th>Item</th>
      <th>Location</th>
      <th>Category</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @physical_items.each do |physical_item| %>
      <tr>
        <td><%= physical_item.id %></td>
        <td><%= physical_item.item_id %></td>
        <td><%= physical_item.location_id %></td>
        <td><%= physical_item.category %></td>
        <td><%= link_to 'Show', [:administrator, physical_item], class: 'btn btn-default btn-xs' %></td>
        <td><%= link_to 'Edit', edit_administrator_physical_item_path(physical_item), class: 'btn btn-default btn-xs' %></td>
        <td><%= link_to 'Destroy', [:administrator, physical_item], method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-xs btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Physical item', new_administrator_physical_item_path, class: 'btn btn-primary' %>
